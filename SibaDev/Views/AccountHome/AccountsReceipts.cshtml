                                                    <input class="form-control" type="text" maxlength="15" id="ARD_TXN_OFF_CODE" name="ARD_TXN_OFF_CODE" title="Transaction Branch Code" required />
                                                    <span class="input-group-btn">
                                                        <button type="button" class="btn btn-default btn_lov" data-dialog='{"type":"btn_txn_office" ,"field1":"ARD_TXN_OFF_CODE", "field2":"TXN_OFF_NAME"}'>
