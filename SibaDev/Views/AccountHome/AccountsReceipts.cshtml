                                            <i class="ico ion-eye text-blue" title="Print" aria-hidden="true"></i> View Receipt
                                        </button>

                                    </div>

                                    <div class="col-md-5">
                                        <button type="button" id="btnQueryReceipt" class="btn btn-info" style="float: right; margin-right: 15px">
                                            <i class="fa fa-search" title="Search" aria-hidden="true"></i> Search
                                        </button>
                                        <input id="txtSearch" style="height: 30px; text-align:center; float:right; margin-right: 10px"
                                               class="text ui-widget-content ui-corner-all" type="text" name="txtSearch" placeholder="Search by Code">
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                    <div class="row form-header">

                        <div class="col-md-9">
                            <div class="row control-group form-padding">
                                <span class="control-grp-lbl text-danger">Batch Details</span>
                                <form name="receiptHeaderForm" id="receiptHeaderForm">
                                    <!--hidden fields-->
                                    <input type="hidden" id="ARH_SYS_ID" name="ARH_SYS_ID" />
                                    <input type="hidden" id="ARH_STATUS" name="ARH_STATUS" value="U" />
                                    <input type="hidden" id="ARH_TRANS_STATUS" name="ARH_TRANS_STATUS" value="P" />

                                    <div class="row">
                                        <div class="form-group">

                                            <div class="col-md-2">
                                                <label>Issuing Office</label>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="input-group">
                                                    <input class="form-control" type="text" id="ARH_OFF_CODE" maxlength="15" name="ARH_OFF_CODE" title="Issuing Office Code" required />
                                                    <span class="input-group-btn">
                                                        <button type="button" class="btn btn-default btn_lov" data-dialog='{"type":"btn_issue_office" ,"field1":"ARH_OFF_CODE", "field2":"OFF_NAME"}'>
                                                            <i class="fa fa-sort-down" title="Select" aria-hidden="true"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <input class="form-control" type="text" name="OFF_NAME" title="Issuing Office Name" />
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="form-group">

                                            <div class="col-md-2">
                                                <label>Document Type</label>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="input-group">
                                                    <input class="form-control" type="text" id="ARH_ATD_CODE" maxlength="15" name="ARH_ATD_CODE" title="Document Type Code" required />
                                                    <span class="input-group-btn">
                                                        <button type="button" class="btn btn-default btn_lov" data-dialog='{"type":"btn_doc_type" ,"field1":"ARH_ATD_CODE", "field2":"ATD_NAME"}'>
                                                            <i class="fa fa-sort-down" title="Select" aria-hidden="true"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <input class="form-control" type="text" name="ATD_NAME" title="Document Type Name" />
                                            </div>

                                        </div>
                                    </div>

                                    <!-- date -->
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-2">
                                                <label>Txn Date</label>
                                            </div>
                                            <div class="col-md-2">
                                                <input class="form-control date" type="text" name="ARH_TXN_DATE" title="Transaction Date" required/>
                                            </div>

                                            <div class="col-md-1">
                                                <label> Batch No</label>
                                            </div>
                                            <div class="col-md-2">
                                                <input class="form-control" type="number" min="1" id="ARH_BATCH_NO" name="ARH_BATCH_NO" title="Receipt Batch Number" required />
                                            </div>
                                        </div>
                                      
                                    </div>
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-2">
                                                <label> Receipt Type</label>
                                            </div>
                                            <div class="col-md-2">
                                                <select id="ARH_RCPT_TYPE" class="form-control" name="ARH_RCPT_TYPE" required>
                                                    <option value=""> --- Select Type --- </option>
                                                    <option value="CR"> Customer Receipts</option>
                                                    <option value="OR"> Other Receipts </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                    
                                    </div>
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-2">
                                                <label>Batch Narration</label>
                                            </div>
                                            <div class="col-md-4">
                                                <textarea class="form-control" maxlength="1000" id="ARH_DESC" name="ARH_DESC" title="Batch Narration" required></textarea>
                                            </div>
                                        </div>
                                    </div>


                                </form>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="row control-group form-padding">
                                <span class="control-grp-lbl text-danger" style="text-align: left">Receipt State</span>
                                <div class="row">
                                    <div class="form-group">
                                        <button type="button" class="center btn btn-default " id="btnProposalConfirmation">
                                            @*<i class="fa ion-checkmark-circled text-success" title="" aria-hidden="true"></i>*@ <span class="transStatusTxt">Confirm</span> Receipt
                                        </button>
                                    </div>

                                    <div class="form-group">
                                        <input type="text" name="ClaimStatus" readonly value="" class="form-control text-center center" placeholder="Receipt Status" />
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                <!--Grid Buttons-->
                <div id="tabs">
                <ul>
                    <li><a href="#tabs-1">Receipts Details</a></li>
                    
                   
                </ul>
                <div id="tabs-1">
                    <div class="row control-group form-padding">
                        <span class="control-grp-lbl text-danger">Receipt Header info</span>

                        <form name="receiptDetailsForm" id="receiptDetailsForm">
                            <!--HIDDEN FIELDS-->
                            <input id="MS_ACCT_RCPT_ACNT_DTLS" type="hidden" name="MS_ACCT_RCPT_ACNT_DTLS" value="[]" />
                            <div class="row">
                                <div class="col-md-6">

                                    <div class="row">
                                        <div class="form-group">

                                            <div class="col-md-3">
                                                <label>Txn Branch</label>
                                            </div>
                                            <div class="col-md-3">
