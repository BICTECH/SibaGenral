                TXN_DOC_NAME: $('#txtTxnName').val(),
                TXN_DOC_DESC: $('#txtTxnDesc').val(),
                TXN_DOC_POST_MODE: $("input[name=radPostMode]:checked").val(), //
                TXN_DOC_PAY_MODE: $("input[name=radPmtMode]:checked").val(),
                TXN_DOC_GEN_NO_YN: ($('#chkGenDoc').is(':checked')) ? 'Y' : 'N',
                TXN_DOC_POST_YN: ($('#chkAppPost').is(':checked')) ? 'Y' : 'N',
                TXN_DOC_REVERSE_YN: ($('#chkReverse').is(':checked')) ? 'Y' : 'N',
                TXN_DOC_AUTHOZE_YN: ($('#chkAuthorize').is(':checked')) ? 'Y' : 'N',
                TXN_DOC_PRNT_B_P_YN: ($('#chkPrintBPost').is(':checked')) ? 'Y' : 'N',
                TXN_DOC_PRNT_A_P_YN: ($('#chkPrintAPost').is(':checked')) ? 'Y' : 'N',
                TXN_DOC_CRTE_BY: "DEMO",
                // TXN_DOC_CRTE_DATE : ,
                TXN_DOC_MOD_BY: "DEMO",
                //TXN_DOC_MOD_DATE : ,
                TXN_DOC_STATUS: "U",

            }];


            var txnNoData = jQuery("#grdAppTxnNo").jqGrid('getRowData');
            var txnDftAcctData = jQuery("#grdDeftAccount").jqGrid('getRowData');



            //alert(JSON.stringify(agtBrokerCommData));

            if ($('#txtTxnState').val() == "") {

                Url = '/api/AcctTransactionDocuments/CreateTxnDocuments';
            }
            else if ($('#txtTxnState').val() == "A") {

                //udate if status is A (Saved and active)
                Url = '/api/AcctTransactionDocuments/UpdateTxnDocuments';
            }
            // --- -- ------

            $.ajax({

                type: 'POST',
                url: Url,

                //Data to send

                data: JSON.stringify({ txnDoc: txnDocHeadData, txnNo: txnNoData, txnAcct: txnDftAcctData }),

                contentType: 'application/json; charset=utf-8',
                dataType: 'json',

                //dataprocess: true,
                success: function (data) {

                    if (data == "A" || data == "F") {
                        callback(data);
                    }
                    else {
                        alert(JSON.stringify(data));
                    }
                },
                'error': function (error) {

                    callback(error);

                    return (error);
                    alert(JSON.stringify(error));
                }
            });
        };

